                                "conditions": [
                                    {
                                        "type": "variable_if",
                                        "name": "launcher_mode",
                                        "value": 1
                                    }
                                ]
                            },
                            {
                                "type": "basic",
                                "from": {"key_code": "t"},
                                "to": [
                                    {
                                        "shell_command": "open -a 'Things3.app'"
                                    }
                                ],
                                "conditions": [
                                    {
                                        "type": "variable_if",
                                        "name": "launcher_mode",
                                        "value": 1
                                    }
                                ]
                            },
                            {
                                "type": "basic",
                                "from": {"key_code": "s"},
                                "to": [
                                    {
                                        "shell_command": "open -a 'Slack.app'"
                                    }
                                ],
                                "conditions": [
                                    {
                                        "type": "variable_if",
                                        "name": "launcher_mode",
                                        "value": 1
                                    }
                                ]
                            },
                            {
                                "type": "basic",
                                "from": {"key_code": "n"},
                                "to": [
                                    {
                                        "shell_command": "open 'things:///add?show-quick-entry=true'"
                                    }
                                ],
                                "conditions": [
                                    {
                                        "type": "variable_if",
                                        "name": "launcher_mode",
                                        "value": 1
                                    }
                                ]
                            }
                        ]
                    }
