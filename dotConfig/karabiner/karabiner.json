{
    "global": {
        "check_for_updates_on_startup": true,
        "show_in_menu_bar": true,
        "show_profile_name_in_menu_bar": false
    },
    "profiles": [
        {
            "complex_modifications": {
                "parameters": {
                    "basic.simultaneous_threshold_milliseconds": 50,
                    "basic.to_delayed_action_delay_milliseconds": 500,
                    "basic.to_if_alone_timeout_milliseconds": 1000,
                    "basic.to_if_held_down_threshold_milliseconds": 500
                },
                "rules": [
                    {
                        "description": "Simultaneous mappings",
                        "manipulators": [
                            {
                                "description": "F+J to <Enter>",
                                "from": {
                                    "modifiers": {
                                        "optional": ["any"]
                                    },
                                    "simultaneous": [
                                        {"key_code": "f"},
                                        {"key_code": "j"}
                                    ]
                                },
                                "to": [
                                    {"key_code": "return_or_enter"}
                                ],
                                "type": "basic"
                            },
                            {
                                "description": "F+K as tmux trigger",
                                "from": {
                                    "simultaneous": [
                                        {"key_code": "f"},
                                        {"key_code": "k"}
                                    ]
                                },
                                "to": [
                                    {
                                        "key_code": "h",
                                        "modifiers": ["left_control"]
                                    }
                                ],
                                "type": "basic"
                            },
                            {
                                "description": "G+J as vim leader",
                                "from": {
                                    "simultaneous": [
                                        {"key_code": "g"},
                                        {"key_code": "j"}
                                    ]
                                },
                                "to": [
                                    {
                                        "key_code": "q",
                                        "modifiers": ["right_control"]
                                    }
                                ],
                                "type": "basic"
                            },
                            {
                                "description": "V+J as ctrl",
                                "from": {
                                    "simultaneous": [
                                        {"key_code": "v"},
                                        {"key_code": "j"}
                                    ]
                                },
                                "to": [
                                    {
                                        "key_code": "left_control",
                                        "lazy": true
                                    }
                                ],
                                "type": "basic"
                            },
                            {
                                "description": "T+J as shift",
                                "from": {
                                    "simultaneous": [
                                        {"key_code": "t"},
                                        {"key_code": "j"}
                                    ]
                                },
                                "to": [
                                    {
                                        "key_code": "left_shift",
                                        "lazy": true
                                    }
                                ],
                                "type": "basic"
                            },
                            {
                                "description": "J+W as backspace",
                                "type": "basic",
                                "from": {
                                    "simultaneous": [
                                        {"key_code": "w"},
                                        {"key_code": "j"}
                                    ] 
                                },
                                "to": [
                                    {
                                        "set_variable": {
                                            "name": "backspace_mode",
                                            "value": 1
                                        }
                                    }
                                ],
                                "to_after_key_up": [
                                    {
                                        "set_variable": {
                                            "name": "backspace_mode",
                                            "value": 0
                                        }
                                    }
                                ]
                            },
                            {
                                "type": "basic",
                                "from": {
                                    "key_code": "f",
                                    "modifiers": {
                                        "optional": ["any"]
                                    }
                                },
                                "to": [
                                    {"key_code": "delete_or_backspace"}
                                ],
                                "conditions": [
                                    {
                                        "type": "variable_if",
                                        "name": "backspace_mode",
                                        "value": 1
                                    }
                                ]
                            },
                            {
                                "type": "basic",
                                "from": {"key_code": "j"},
                                "to": [{"key_code": "delete_or_backspace"}],
                                "conditions": [
                                    {
                                        "type": "variable_if",
                                        "name": "backspace_mode",
                                        "value": 1
                                    }
                                ]
                            },
                            {
                                "description": "disable return",
                                "from": {"key_code": "return_or_enter"},
                                "type": "basic"
                            },
                            {
                                "description": "disable delete",
                                "from": {"key_code": "delete_or_backspace"},
                                "type": "basic"
                            },
                            {
                                "description": "disable left_shift",
                                "from": {"key_code": "left_shift"},
                                "type": "basic"
                            },
                            {
                                "description": "disable right_shift",
                                "from": {"key_code": "right_shift"},
                                "type": "basic"
                            },
                            {
                                "type": "basic",
                                "from": {
                                    "simultaneous": [
                                        {"key_code": "h"},
                                        {"key_code": "v"}
                                    ] 
                                },
                                "to": [
                                    {
                                        "set_variable": {
                                            "name": "launcher_mode",
                                            "value": 1
                                        }
                                    }
                                ],
                                "to_after_key_up": [
                                    {
                                        "set_variable": {
                                            "name": "launcher_mode",
                                            "value": 0
                                        }
                                    }
                                ]
                            },
                            {
                                "type": "basic",
                                "from": {"key_code": "c"},
                                "to": [
                                    {
                                        "shell_command": "open -a 'Google Chrome.app'"
                                    }
                                ],
                                "conditions": [
                                    {
                                        "type": "variable_if",
                                        "name": "launcher_mode",
                                        "value": 1
                                    }
                                ]
                            },
                            {
                                "type": "basic",
                                "from": {"key_code": "a"},
