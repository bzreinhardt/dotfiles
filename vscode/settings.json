{
    "files.defaultLanguage": "markdown",
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "vscode-neovim.neovimExecutablePaths.darwin": "/Users/pokey/nvim-osx64/bin/nvim",
    "vscode-neovim.neovimInitVimPaths.darwin": "/Users/pokey/.config/nvim/vscode-init.vim",
    "multiCommand.commands": [
        {
            "command": "multiCommand.exitAndSave",
            "sequence": [
                "vscode-neovim.escape",
                "workbench.action.files.save",
            ]
        },
        {
            "command": "multiCommand.commandMode",
            "sequence": [
                "vscode-neovim.escape",
                "vscode-neovim.enable",
            ]
        }
    ],
    "workbench.colorTheme": "Abyss",
    "terminal.integrated.fontFamily": "SF Mono, Menlo, Monaco, 'Courier New', monospace",
    "terminal.integrated.fontSize": 13,
    "editor.formatOnSave": true,
    "editor.lineNumbers": "off",
    "editor.glyphMargin": false,
    "workbench.editor.showTabs": false,
    "workbench.activityBar.visible": false,
    "editor.lineDecorationsWidth": 0, // undocumented
    "editor.lineNumbersMinChars": 0,
    "[typescript]": {
        "editor.formatOnSave": false,
        "editor.defaultFormatter": "esbenp.prettier-vscode",
        "editor.codeActionsOnSave": [
            "source.fixAll.format",
            "source.fixAll.eslint"
        ],
    },
    "[typescriptreact]": {
        "editor.formatOnSave": false,
        "editor.defaultFormatter": "esbenp.prettier-vscode",
        "editor.codeActionsOnSave": [
            "source.fixAll.format",
            "source.fixAll.eslint"
        ],
    },
    "workbench.preferredDarkColorTheme": "Abyss",
    "workbench.colorCustomizations": {
        "[Abyss]": {
            "editorError.foreground": "#b32222",
            "editor.background": "#000000",
            "statusBar.foreground": "#767a7e",
            "editor.foreground": "#bec0c4",
            "diffEditor.insertedTextBackground": "#00cf341c",
            "editor.wordHighlightBackground": "#ffffff2b",
            "editor.wordHighlightStrongBackground": "#ffffff2b",
            "editor.rangeHighlightBackground": "#ffffff2b",
            "editor.selectionHighlightBackground": "#ffffff2b",
            "cursorless.pendingDeleteBackground": "#ff00008a",
            "cursorless.referencedBackground": "#00a2ff4d",
            "cursorless.justAddedBackground": "#09ff005b",
            "cursorless.pendingModification0Background": "#8c00ff86",
            "cursorless.pendingModification1Background": "#ff009d7e",
        }
    },
    "editor.tokenColorCustomizations": {
        "[Abyss]": {
            "keywords": "#be0000",
            "variables": "#78a1f3",
            "comments": "#5066be"
        },
        "textMateRules": [
            {
                "scope": "support.type",
                "settings": {
                    "fontStyle": ""
                }
            },
            {
                "scope": "keyword.operator",
                "settings": {
                    "foreground": "#bdbdbd"
                }
            },
            {
                "scope": "storage.type",
                "settings": {
                    "fontStyle": ""
                }
            },
            {
                "scope": "variable.parameter",
                "settings": {
                    "fontStyle": ""
                }
            },
            {
                "scope": "support.class",
                "settings": {
                    "fontStyle": ""
                }
            }
        ]
    },
    "[jsonc]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "python.languageServer": "Pylance",
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter-notebook"
    },
    "window.nativeFullScreen": false,
    "window.newWindowDimensions": "fullscreen",
    "window.restoreFullscreen": true,
    "editor.scrollbar.vertical": "visible", // undocumented
    "window.titleBarStyle": "custom",
    "gitlens.advanced.fileHistoryFollowsRenames": true,
    "gitlens.codeLens.scopes": [
        "document"
    ],
    "jupyter.alwaysTrustNotebooks": true,
    "editor.minimap.scale": 3,
    "files.associations": {
        "rc": "shellscript",
        "Fastfile": "ruby"
    },
    "[handlebars]": {
        "editor.formatOnSave": false
    },
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "workbench.iconTheme": "vscode-icons",
    // Controls whether suggestions should automatically show up while typing.
    "editor.quickSuggestions": {
        "other": true,
        "comments": true,
        "strings": true
    },
    "git.enableSmartCommit": true,
    "git.confirmSync": false,
    "typescript.updateImportsOnFileMove.enabled": "always",
    "cSpell.userWords": [
        "Subtokens",
        "cursorless",
        "deserialization",
        "esbuild",
        "itertools",
        "jsonl",
        "nocheck",
        "outfile",
        "subpiece",
        "subtoken",
        "subwords",
        "unmount"
    ],
    "terminal.integrated.scrollback": 100000,
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "cSpell.showStatus": false,
    "workbench.editor.enablePreview": true,
    "diffEditor.ignoreTrimWhitespace": false,
    "customizeUI.activityBar": "regular",
    "customizeUI.stylesheet": {
        ".title-actions:hover .actions-container": "display: flex !important;",
        ".title-actions .actions-container": "display: none !important;",
    },
    "vscode-neovim.textDecorationsAtTop": true,
    "javascript.suggest.completeFunctionCalls": true,
    "typescript.suggest.completeFunctionCalls": true,
    "typescript.tsserver.experimental.enableProjectDiagnostics": true,
    "vsintellicode.features.python.deepLearning": "enabled",
    "javascript.updateImportsOnFileMove.enabled": "always",
    "editor.codeLensFontSize": 10,
    "references.preferredLocation": "view",
    "window.openFoldersInNewWindow": "on",
    "cursorless.showOnStart": true,
    "cursorless.showAdditionalHighlightBeforeScroll": false,
    "cursorless.pendingEditDecorationTime": 100,
    "cursorless.colors.red": {
        "dark": "#db3e00",
    },
    "cursorless.colors.purple": {
        "dark": "#fa76ff",
    },
    "cursorless.hatStyles.curve.enabled": true,
    "cursorless.hatStyles.threePointStar.enabled": true,
    "cursorless.hatStyles.hole.enabled": true,
    "cursorless.hatStyles.frame.enabled": true,
    "cursorless.hatStyles.eye.enabled": true,
    "cursorless.hatStyles.yellow.penalty": 2,
    "editor.accessibilitySupport": "off",
    "tabnine.experimentalAutoImports": true,
    "editor.acceptSuggestionOnEnter": "off",
    "editor.autoClosingBrackets": "beforeWhitespace",
    "editor.autoClosingQuotes": "beforeWhitespace",
    "vscode-neovim.logOutputToConsole": true,
    "vscode-neovim.logLevel": "debug",
    "debug.onTaskErrors": "debugAnyway",
    "jupyter.askForKernelRestart": false,
    "markdown.extension.toc.levels": "2..6",
    "editor.fontFamily": "SF Mono, Menlo, Monaco, 'Courier New', monospace",
    "editor.fontSize": 13,
    "notebook.cellToolbarLocation": {
        "default": "right",
        "jupyter-notebook": "left"
    },
    "jupyter.enableNativeInteractiveWindow": true,
    "window.zoomLevel": 1,
    "vsicons.dontShowNewVersionMessage": true,
    "[markdown]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "githubPullRequests.fileListLayout": "tree",
    "ruby.useBundler": true, //run non-lint commands with bundle exec
    "ruby.useLanguageServer": true, // use the internal language server (see below)
    "ruby.lint": {
        "rubocop": {
            "useBundler": true // enable rubocop via bundler
        },
        "reek": {
            "useBundler": true // enable reek via bundler
        }
    },
    "ruby.format": "rubocop",
    "explorer.confirmDragAndDrop": false,
    "editor.minimap.enabled": false, // use rubocop for formatting
}