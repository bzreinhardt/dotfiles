    "editor.tokenColorCustomizations": {
        "[Abyss]": {
            "keywords": "#be0000",
            "variables": "#78a1f3",
            "comments": "#5066be"
        },
        "textMateRules": [
            {
                "scope": "support.type",
                "settings": {
                    "fontStyle": ""
                }
            },
            {
                "scope": "keyword.operator",
                "settings": {
                    "foreground": "#bdbdbd"
                }
            },
            {
                "scope": "storage.type",
                "settings": {
                    "fontStyle": ""
                }
            },
            {
                "scope": "variable.parameter",
                "settings": {
                    "fontStyle": ""
                }
            },
            {
                "scope": "support.class",
                "settings": {
                    "fontStyle": ""
                }
            }
        ]
    },
    "[jsonc]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "python.languageServer": "Pylance",
    "workbench.editorAssociations": [
        {
            "viewType": "jupyter.notebook.ipynb",
            "filenamePattern": "*.ipynb"
        }
    ],
    "window.nativeFullScreen": false,
    "window.newWindowDimensions": "fullscreen",
    "window.restoreFullscreen": true,
    "editor.scrollbar.vertical": "visible", // undocumented
    "window.titleBarStyle": "native",
    "gitlens.advanced.fileHistoryFollowsRenames": true,
    "gitlens.codeLens.scopes": [
        "document"
    ],
    "jupyter.alwaysTrustNotebooks": true,
    "editor.minimap.scale": 3,
    "files.associations": {
        "rc": "shellscript",
        "Fastfile": "ruby"
    },
    "[handlebars]": {
        "editor.formatOnSave": false
    },
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "workbench.iconTheme": "vscode-icons",
    // Controls whether suggestions should automatically show up while typing.
    "editor.quickSuggestions": {
        "other": true,
        "comments": true,
        "strings": true
    },
    "git.enableSmartCommit": true,
    "git.confirmSync": false,
    "typescript.updateImportsOnFileMove.enabled": "always",
    "cSpell.userWords": [
        "cursorless",
        "deserialization",
        "esbuild",
        "itertools",
        "jsonl",
        "nocheck",
        "outfile",
        "subpiece",
        "subtoken",
        "unmount"
    ],
    "terminal.integrated.scrollback": 100000,
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "cSpell.showStatus": false,
    "workbench.editor.enablePreview": true,
    "diffEditor.ignoreTrimWhitespace": false,
    "customizeUI.activityBar": "regular",
    "customizeUI.stylesheet": {
        ".title-actions:hover .actions-container": "display: flex !important;",
        ".title-actions .actions-container": "display: none !important;",
    },
    "vscode-neovim.textDecorationsAtTop": true,
    "javascript.suggest.completeFunctionCalls": true,
    "typescript.suggest.completeFunctionCalls": true,
    "typescript.tsserver.experimental.enableProjectDiagnostics": true,
    "vsintellicode.features.python.deepLearning": "enabled",
    "javascript.updateImportsOnFileMove.enabled": "always",
    "editor.codeLensFontSize": 10,
    "references.preferredLocation": "view",
    "window.openFoldersInNewWindow": "on",
    "cursorless.showOnStart": true,
    "cursorless.pendingEditDecorationTime": 100,
    "editor.accessibilitySupport": "off",
    "tabnine.experimentalAutoImports": true,
    "editor.acceptSuggestionOnEnter": "off",
    "editor.autoClosingBrackets": "beforeWhitespace",
    "editor.autoClosingQuotes": "beforeWhitespace",
    "vscode-neovim.logOutputToConsole": true,
    "vscode-neovim.logLevel": "debug",
    "debug.onTaskErrors": "debugAnyway"
}